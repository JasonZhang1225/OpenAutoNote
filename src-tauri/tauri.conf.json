{
    "productName": "OpenAutoNote",
    "version": "1.0.0",
    "identifier": "com.openautonote.app",
    "build": {
        "beforeDevCommand": "cd python-core && ./oan2/bin/python main.py",
        "beforeBuildCommand": "",
        "devUrl": "http://localhost:8964",
        "frontendDist": "../dist-python"
    },
    "bundle": {
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ],
        "active": true,
        "targets": "all",
        "externalBin": [
            "bin/api-server"
        ],
        "windows": {
            "nsis": {
                "compression": "none"
            }
        }
    },
    "app": {
        "withGlobalTauri": true,
        "security": {
            "csp": null
        }
    }
}